<jfd:form xmlns:jfd="http://xmlns.m-g.ru/jet/ui" type="maintenance" orientation="ver"
          verticalGap="5" controller="com.mg.framework.generic.ui.DefaultMaintenanceForm"
          title="resource://com.mg.merp.account.resources.formelements#EconomicSpecModel.FormMt.Title"
          helpTopic="account#econospec_html">

  <jfd:include name="com/mg/framework/resources/DefaultMaintenanceHeaderMacros.xml"/>

  <jfd:container id="fields" layout="jfd:tabbed" alignment="expand_expand">

    <jfd:container id="commonFields" layout="jfd:defaultMtLayout"
                   tabTitle="resource://com.mg.merp.account.resources.formelements#EconSpecModel.Total.Tab">
      <jfd:include name="com/mg/merp/account/resources/AccDebetMacros.mfd.xml"/>
      <jfd:include name="com/mg/merp/account/resources/AccKreditMacros.mfd.xml"/>
      <jfd:field id="AccDb.Currency"/>
      <!--jfd:field id="CurCource" sameLine="true"/-->
      <jfd:field id="SummaCur"/>
      <jfd:field id="SummaNat" sameLine="true"/>
      <jfd:field id="SumAlg"/>
      <jfd:field id="QtyAlg" sameLine="true"/>
      <jfd:include name="com/mg/framework/resources/DefaultMaintenanceAddinFieldMacros.xml"/>
    </jfd:container>
    <jfd:container id="featureFields" alignment="expand_top" layout="jfd:defaultMtLayout"
                   tabTitle="resource://com.mg.merp.account.resources.formelements#EconSpec.Feature">
      <jfd:field id="Feature1"/>
      <jfd:field id="Feature2"/>
      <jfd:field id="Feature3"/>
      <jfd:field id="Feature4"/>
      <jfd:field id="Feature5"/>
    </jfd:container>
    <jfd:container id="entryFormulaFields" layout="jfd:defaultMtLayout" alignment="expand_top"
                   tabTitle="resource://com.mg.merp.account.resources.formelements#EconSpecModel.Entry.Tab">
      <jfd:field id="EntryFolder"/>
      <jfd:field id="EntryGoodType"/>
      <jfd:field id="Catalog"/>
      <jfd:field id="TaxGroup"/>
    </jfd:container>
  </jfd:container>

  <jfd:include name="com/mg/framework/resources/DefaultMaintenanceFooterMacros.xml"/>

</jfd:form>
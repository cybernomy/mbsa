<jfd:form xmlns:jfd="http://xmlns.m-g.ru/jet/ui"
          controller="com.mg.merp.planning.support.ui.MRPVersionControlRest" orientation="ver"
          verticalGap="5"
          title="resource://com.mg.merp.planning.resources.formelements#Cond.MRPVersion"
          helpTopic="planning#mrpvers_html">

  <jfd:include name="com/mg/framework/resources/DefaultHierarchyRestrictionHeaderMacros.xml"/>

  <jfd:container id="fields" layout="jfd:defaultMtGridBag" alignment="expand_expand">
    <jfd:border style="bevel" type="raised"/>

    <jfd:field id="startDateFrom"/>
    <jfd:field id="startDateTill" sameLine="true"/>
    <jfd:field id="endDateFrom"/>
    <jfd:field id="endDateTill" sameLine="true"/>
    <jfd:field id="mpsCode"/>
    <jfd:field id="forecastVersionCode" sameLine="true"/>
    <jfd:include name="com/mg/framework/resources/DefaultMaintenanceAddinFieldMacros.xml"/>
  </jfd:container>

  <jfd:include name="com/mg/framework/resources/DefaultRestrictionFooterMacros.xml"/>

</jfd:form>
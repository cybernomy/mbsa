<jfd:form xmlns:jfd="http://xmlns.m-g.ru/jet/ui" type="maintenance" orientation="ver"
          verticalGap="5" controller="com.mg.framework.generic.ui.DefaultMaintenanceForm"
          title="resource://com.mg.merp.reference.resources.formelements#Bank.FormBr.Title"
          helpTopic="reference#banks_html">

  <jfd:include name="com/mg/framework/resources/DefaultMaintenanceHeaderMacros.xml"/>

  <jfd:container id="fields" layout="jfd:defaultMtGridBag" alignment="expand_expand">
    <jfd:border style="bevel" type="raised"/>
    <jfd:field id="Iban"/>
    <jfd:field id="Swift"/>
    <jfd:field id="Bsc" sameLine="true"/>
    <jfd:field id="BIK"/>
    <jfd:field id="BikUnique" sameLine="true"/>


    <jfd:field id="Name"/>
    <jfd:field id="Branch"/>

    <jfd:field id="Country" longField="false"/>
    <jfd:field id="Zip" sameLine="true"/>
    <jfd:field id="CityType"/>
    <jfd:field id="City"/>

    <jfd:field id="Address"/>
    <jfd:field id="CorrAcc"/>
    <jfd:field id="Currency" sameLine="true"/>
    <jfd:field id="CorrName"/>
    <jfd:field id="CorrAddress"/>
    <jfd:field id="Phone"/>
    <jfd:field id="Fax"/>
    <jfd:field id="Email"/>
    <jfd:field id="Www"/>
    <jfd:field id="Comments"/>
    <jfd:include name="com/mg/framework/resources/DefaultMaintenanceAddinFieldMacros.xml"/>
  </jfd:container>

  <jfd:include name="com/mg/framework/resources/DefaultMaintenanceFooterMacros.xml"/>

</jfd:form>
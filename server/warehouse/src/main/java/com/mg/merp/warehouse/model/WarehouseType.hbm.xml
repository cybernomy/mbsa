<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

  <class name="com.mg.merp.warehouse.model.WarehouseType" table="WH_WAREHOUSE_TYPE">
    <meta attribute="extends">com.mg.framework.service.PersistentObjectHibernate</meta>
    <id name="Id" type="java.lang.Integer">
      <column name="ID" not-null="false" unique="true" sql-type="INTEGER" />
      <generator class="sequence">
        <param name="sequence">WH_WAREHOUSE_TYPE_ID_GEN</param>
      </generator>
    </id>
    <many-to-one name="SysClient" class="com.mg.merp.core.model.SysClient">
      <column name="CLIENT_ID" not-null="false" />
    </many-to-one>    
    <property name="Code" type="java.lang.String">
      <column name="CODE" length="20" not-null="true" sql-type="CHAR" />
    </property>
    <property name="Name" type="java.lang.String">
      <column name="NAME" length="80" not-null="true" sql-type="VARCHAR" />
    </property>
    <filter name="__mg_tenantFilter"/>
  </class> 
</hibernate-mapping>
  


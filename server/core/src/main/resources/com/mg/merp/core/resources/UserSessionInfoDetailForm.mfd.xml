<jfd:form xmlns:jfd="http://xmlns.m-g.ru/jet/ui" controller="com.mg.merp.core.support.ui.UserSessionInfoDetailForm" width="600" height="250" title="resource://com.mg.merp.core.resources.formelements#UserSessionInfoDetailForm.Title" orientation="ver" verticalGap="5" helpTopic="core#user_session_detail_html" >


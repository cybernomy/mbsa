<jfd:form xmlns:jfd="http://xmlns.m-g.ru/jet/ui" type="maintenance" orientation="ver"
          verticalGap="5" controller="com.mg.framework.generic.ui.DefaultMaintenanceForm"
          title="resource://com.mg.merp.mfreference.resources.formelements#Route.FormMt.Title"
          helpTopic="mfreference#route_html">

  <jfd:include name="com/mg/framework/resources/DefaultMaintenanceHeaderMacros.xml"/>

  <jfd:container id="fields" layout="jfd:defaultMtGridBag" alignment="expand_expand">
    <jfd:border style="bevel" type="raised"/>

    <jfd:field id="Catalog"/>
    <jfd:field id="SrcType" editorType="comboBox"/>
    <jfd:field id="DestType" editorType="comboBox" sameLine="true"/>
    <jfd:field id="Vendor"/>
    <jfd:field id="Customer" sameLine="true"/>
    <jfd:field id="SrcWarehouse"/>
    <jfd:field id="DestWarehouse" sameLine="true"/>
    <jfd:field id="SrcCycle"/>
    <jfd:field id="DestCycle" sameLine="true"/>
    <jfd:field id="SrcRank"/>
    <jfd:field id="SupplyPercent" sameLine="true"/>
    <jfd:field id="ReorderMinQty"/>
    <jfd:field id="ReorderLotSize" sameLine="true"/>
    <jfd:field id="LeadTime"/>
    <jfd:field id="SafetyDays" sameLine="true"/>
    <jfd:field id="QcReceivingDays"/>
    <jfd:include name="com/mg/framework/resources/DefaultMaintenanceAddinFieldMacros.xml"/>

  </jfd:container>

  <jfd:include name="com/mg/framework/resources/DefaultMaintenanceFooterMacros.xml"/>

</jfd:form>
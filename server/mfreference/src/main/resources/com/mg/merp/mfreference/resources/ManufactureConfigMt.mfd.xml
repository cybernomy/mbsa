    <jfd:field id="ScrapLaborModel" sameLine="true"/>
    <jfd:field id="ScrapMachineModel"/>

    <jfd:field id="MaterialOverhead"
               fieldGroup="resource://com.mg.merp.mfreference.resources.formelements#ManufactureConfig.FormMt.OverheadFields"
               text="resource://com.mg.merp.mfreference.resources.formelements#ManufactureConfig.FormMt.MaterialOverhead"/>
    <jfd:field id="LaborOverhead" sameLine="true"
               text="resource://com.mg.merp.mfreference.resources.formelements#ManufactureConfig.FormMt.LaborOverhead"/>
    <jfd:field id="MachineOverhead"
               text="resource://com.mg.merp.mfreference.resources.formelements#ManufactureConfig.FormMt.MachineOverhead"/>


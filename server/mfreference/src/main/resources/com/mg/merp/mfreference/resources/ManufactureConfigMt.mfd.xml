               fieldGroup="resource://com.mg.merp.mfreference.resources.formelements#ManufactureConfig.FormMt.InputDocumentModelFields"/>
    <jfd:field id="InputLaborModelBf" sameLine="true"/>
    <jfd:field id="InputMachineModelBf"/>

    <jfd:field id="PurchaseOrderModelMrpRec"
               fieldGroup="resource://com.mg.merp.mfreference.resources.formelements#ManufactureConfig.FormMt.OtherGroupFields"/>
    <jfd:field id="VarianceDocumentModel" sameLine="true"/>

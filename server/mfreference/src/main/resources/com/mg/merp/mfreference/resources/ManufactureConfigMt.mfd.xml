<jfd:form xmlns:jfd="http://xmlns.m-g.ru/jet/ui" type="maintenance" orientation="ver"
          verticalGap="5" controller="com.mg.framework.generic.ui.DefaultMaintenanceForm"
          title="resource://com.mg.merp.mfreference.resources.formelements#ManufactureConfig.FormMt.Title">

  <jfd:include name="com/mg/framework/resources/DefaultMaintenanceHeaderMacros.xml"/>
  <jfd:container id="allFields" layout="jfd:defaultMtLayout" alignment="expand_expand">
    <jfd:border style="bevel" type="raised"/>
    <jfd:field id="BaseCurrency" alignment="expand_expand"
               fieldGroup="resource://com.mg.merp.mfreference.resources.formelements#ManufactureConfig.FormMt.CurrencyFields"
               text="resource://com.mg.merp.mfreference.resources.formelements#ManufactureConfig.FormMt.BaseCurrency"/>
    <jfd:field id="CurrencyRateType" sameLine="true"/>
    <jfd:field id="NatCurrency"
               text="resource://com.mg.merp.mfreference.resources.formelements#ManufactureConfig.FormMt.NatCurrency"/>
    <jfd:field id="CurrencyRateAuthority" sameLine="true"/>
    <jfd:field id="CurrencyPrec"
               text="resource://com.mg.merp.mfreference.resources.formelements#ManufactureConfig.FormMt.CurrencyPrec"/>

    <jfd:field id="TickUM"
               fieldGroup="resource://com.mg.merp.mfreference.resources.formelements#ManufactureConfig.FormMt.MeasureGroupFields"
               sameLine="true"
               text="resource://com.mg.merp.mfreference.resources.formelements#ManufactureConfig.FormMt.Measure"/>

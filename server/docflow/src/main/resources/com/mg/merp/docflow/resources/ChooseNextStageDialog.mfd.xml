<!-- $Id: ChooseNextStageDialog.mfd.xml,v 1.7 2008/01/24 12:04:37 safonov Exp $ -->

<jfd:form xmlns:jfd="http://xmlns.m-g.ru/jet/ui" type="browse" controller="com.mg.merp.docflow.support.ui.ChooseNextStageDialog" orientation="ver" verticalGap="5" width="450" height="250" >

	<jfd:container id="stagesBox" layout="jfd:box" alignment="expand_expand">
		<jfd:table id="stagesList" alignment="expand_expand" scroll="true" autoResize="subsequent_columns" selectionMode="singleSelection" sortedModel="false" />
	</jfd:container>

	<jfd:container id="buttons" layout="jfd:box" alignment="expand_top">
		<jfd:label id="processDateLabel" text="resource://com.mg.merp.docflow.resources.formelements#ProcessDateLabel" alignment="left_top" />
		<jfd:dateField id="processDate" alignment="left_top" />
		<jfd:filler id="buttonsFiller" alignment="expand_top"
			type="horizontalGlue" />
		<jfd:button id="OkButton" alignment="right_top" text="resource://com.mg.framework.resources.messages#OkButton"
			actionListener="Ok" />
		<jfd:button id="CancelButton" alignment="right_top" text="resource://com.mg.framework.resources.messages#CancelButton"
			actionListener="Cancel" />
	</jfd:container>

</jfd:form>
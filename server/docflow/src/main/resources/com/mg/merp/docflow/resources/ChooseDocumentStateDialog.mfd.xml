<!-- $Id: ChooseDocumentStateDialog.mfd.xml,v 1.5 2006/12/12 14:55:50 safonov Exp $ -->

<jfd:form xmlns:jfd="http://xmlns.m-g.ru/jet/ui" controller="com.mg.merp.docflow.support.ui.ChooseDocumentStateDialog" orientation="ver" verticalGap="5" title="resource://com.mg.merp.docflow.resources.formelements#ChooseDocumentStateDialog.Title" >

	<jfd:container id="statesBox" layout="jfd:box" alignment="expand_expand">
		<jfd:table id="statesList" alignment="expand_expand" scroll="true" autoResize="subsequent_columns" >
			<jfd:columns>
				<jfd:column fieldName="ReadyAmount" title="resource://com.mg.merp.docflow.resources.formelements#DocActionHistoryTable.ReadyAmount.Title" />
				<jfd:column fieldName="DateTime" title="resource://com.mg.merp.docflow.resources.formelements#DocActionHistoryTable.DateTime.Title" />
				<jfd:column fieldName="UserName" title="resource://com.mg.merp.docflow.resources.formelements#DocActionHistoryTable.UserName.Title" />
				<jfd:column fieldName="StageResult" title="resource://com.mg.merp.docflow.resources.formelements#DocActionHistoryTable.StageResult.Title" />
			</jfd:columns>
		</jfd:table>
	</jfd:container>

	<jfd:container id="buttons" layout="jfd:box" alignment="expand_top">
		<jfd:filler id="buttonsFiller" alignment="expand_top"
			type="horizontalGlue" />
		<jfd:button id="OkButton" alignment="right_top" text="resource://com.mg.framework.resources.messages#OkButton"
			actionListener="Ok" />
		<jfd:button id="CancelButton" alignment="right_top" text="resource://com.mg.framework.resources.messages#CancelButton"
			actionListener="Cancel" />
	</jfd:container>

</jfd:form>
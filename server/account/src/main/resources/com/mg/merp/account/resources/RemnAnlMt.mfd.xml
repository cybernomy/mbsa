    <jfd:field id="RemnEndNatDb"/>
    <jfd:field id="RemnEndNatKt" sameLine="true"/>
    <jfd:field id="RemnBeginCurDb"/>
    <jfd:field id="RemnBeginCurKt" sameLine="true"/>

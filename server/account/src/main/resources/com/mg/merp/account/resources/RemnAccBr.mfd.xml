				</jfd:maintenanceTable>
				<jfd:maintenanceTable id="turnKt" alignment="expand_expand" scroll="true" tabTitle="resource://com.mg.merp.account.resources.formelements#ViewTurns.TurnKt">
				</jfd:maintenanceTable>
				<jfd:maintenanceTable id="turn" alignment="expand_expand" scroll="true" tabTitle="resource://com.mg.merp.account.resources.formelements#ViewTurns.Turn">
				</jfd:maintenanceTable>

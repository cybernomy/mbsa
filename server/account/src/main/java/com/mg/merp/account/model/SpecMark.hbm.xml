<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.mg.merp.account.model.SpecMark" table="SPECMARK">
    <meta attribute="extends">com.mg.framework.service.PersistentObjectHibernate</meta>
    <id name="Id" type="java.lang.Integer">
      <column name="ID" not-null="false" unique="true" sql-type="INTEGER" />
      <generator class="sequence">
        <param name="sequence">SPECMARK_ID_GEN</param>
      </generator>
    </id>
    <many-to-one name="SysClient" class="com.mg.merp.core.model.SysClient">
      <column name="CLIENT_ID" not-null="false" />
    </many-to-one>
    <property name="Code" type="java.lang.String">
      <column name="CODE" length="20" not-null="true" sql-type="CHAR" />
    </property>
    <property name="SmName" type="java.lang.String">
      <column name="SMNAME" length="80" not-null="true" sql-type="VARCHAR" />
    </property>
    <property name="UpCode" type="java.lang.String">
      <column name="UPCODE" length="20" not-null="true" unique="true" sql-type="CHAR" />
    </property>
    <filter name="__mg_tenantFilter"/>
  </class> 
</hibernate-mapping>

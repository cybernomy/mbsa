              toolTip="resource://com.mg.framework.resources.messages#SetupTableButton"
              icon="com/mg/framework/resources/setuptablebuttonicon.png"
              actionListener="SetupTableDetail"/>
  <jfd:separator id="printDetailSeparator"/>
  <jfd:button id="printDetailButton"
              toolTip="resource://com.mg.framework.resources.messages#PrintButton"
              icon="com/mg/framework/resources/printbuttonicon.png"
              actionListener="PrintReportDetail"/>
</jfd:wrap-macros>
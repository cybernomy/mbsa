<jfd:wrap-macros xmlns:jfd="http://xmlns.m-g.ru/jet/ui">
  <jfd:button id="addButton" toolTip="resource://com.mg.framework.resources.messages#AddButton"
              icon="com/mg/framework/resources/addbuttonicon.png" actionListener="AddEntity"/>
  <jfd:button id="editButton" toolTip="resource://com.mg.framework.resources.messages#EditButton"
              icon="com/mg/framework/resources/editbuttonicon.png" actionListener="EditEntity"/>
  <jfd:button id="eraseButton" toolTip="resource://com.mg.framework.resources.messages#EraseButton"
              icon="com/mg/framework/resources/erasebuttonicon.png" actionListener="EraseEntity"/>
  <jfd:button id="viewButton" toolTip="resource://com.mg.framework.resources.messages#ViewButton"
              icon="com/mg/framework/resources/viewbuttonicon.png" actionListener="ViewEntity"/>
  <jfd:button id="cloneButton" toolTip="resource://com.mg.framework.resources.messages#CloneButton"
              icon="com/mg/framework/resources/clonebuttonicon.png" actionListener="CloneEntity"/>
  <jfd:button id="deepCloneButton"
              toolTip="resource://com.mg.framework.resources.messages#DeepCloneButton"
              icon="com/mg/framework/resources/deepclonebuttonicon.png"
              actionListener="DeepCloneEntity"/>
  <jfd:separator id="maintenanceSeparator"/>
  <jfd:button id="refreshButton"
              toolTip="resource://com.mg.framework.resources.messages#RefreshButton"
              icon="com/mg/framework/resources/refreshbuttonicon.png"
              actionListener="RefreshBrowse"/>
  <jfd:button id="restrictionButton"
              toolTip="resource://com.mg.framework.resources.messages#RestrictionButton"
              icon="com/mg/framework/resources/restbuttonicon.png"
              actionListener="SetupRestriction"/>
  <jfd:button id="setupTableButton"
              toolTip="resource://com.mg.framework.resources.messages#SetupTableButton"
              icon="com/mg/framework/resources/setuptablebuttonicon.png"
              actionListener="SetupTable"/>
  <jfd:separator id="printSeparator"/>
  <jfd:button id="printButton" toolTip="resource://com.mg.framework.resources.messages#PrintButton"
              icon="com/mg/framework/resources/printbuttonicon.png" actionListener="PrintReport"/>
</jfd:wrap-macros>